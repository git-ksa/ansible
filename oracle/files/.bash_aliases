alias pcpu='ps -eo pcpu,pid,user,command |sort -nrk 1 | head -20'
alias pmem='ps -eo pmem,pid,user,command |sort -nrk 1 | head -20'
alias asmcmd='rlwrap asmcmd'
alias sqlplus='rlwrap sqlplus'
alias dgmgrl='rlwrap dgmgrl'
alias rman'=rlwrap rman'
