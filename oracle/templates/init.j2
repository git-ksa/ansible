*.__oracle_base='{{ oracle_database_oracle_base }}'#ORACLE_BASE set from environment
*._awr_disabled_flush_tables='WRH$_TEMPSTATXS,WRH$_IOSTAT_FILETYPE'
*.archive_lag_target=0
*.audit_file_dest='/u01/app/oracle/admin/{{ oracle_database_oracle_sid }}/adump'
*.audit_trail='none'
*.compatible='{{ oracle_database_release | replace('ee','') }}.0'
*.control_files='/u01/oradata/{{ oracle_database_oracle_sid }}/control01.ctl','/u02/oradata/{{ oracle_database_oracle_sid }}/control02.ctl'
*.cursor_sharing='FORCE'
*.db_block_size=8192
*.db_domain=''
*.db_name='{{ oracle_database_oracle_sid }}'
#*.db_recovery_file_dest_size=64G
#*.db_recovery_file_dest='/u03/archivelog/ELKDB_STB'
*.db_unique_name='{{ oracle_database_oracle_sid }}'
*.diagnostic_dest='/u01/app/oracle'
*.filesystemio_options='DIRECTIO'
*.LOG_ARCHIVE_FORMAT='%t_%s_%r.dbf'
*.log_archive_format='%t_%s_%r.dbf'
*.log_archive_max_processes=4
*.log_archive_min_succeed_dest=1
*.log_archive_trace=0
*.open_cursors=300
*.optimizer_index_caching=50
*.optimizer_index_cost_adj=5
*.parallel_max_servers=300
*.pga_aggregate_target=250M
*.processes=500
*.remote_login_passwordfile='EXCLUSIVE'
*.db_cache_size=125M
*.sga_max_size=700M
*.sga_target=700M
*.shared_pool_size=250M
*.java_pool_size=30M
*.large_pool_size=60M
*.__shared_io_pool_size=0
*.shared_pool_size=10M
*.streams_pool_size=30M
*.result_cache_max_size=128M
*.result_cache_mode='MANUAL'
*.sessions=500
*.standby_file_management='AUTO'
*.undo_management='AUTO'
*.undo_retention=6000
*.undo_tablespace='UNDOTBS1'
